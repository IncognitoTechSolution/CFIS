<div class="nested-fields">
  <div class ="row">
  <div class="col-md-3">
    <%= f.label :prefix_id %><br>
    <%= f.number_field :prefix_id %>
  </div>
  <div class="col-md-3">
    <%= f.label :entity_type_id, 'Lender Type' %><br>
    <%= f.number_field :entity_type_id %>
  </div>
  <div class="col-md-3">
    <%= f.label :lendername, 'Lender Name' %><br>
    <%= f.text_field :lendername %>
  </div>
  <div class="col-md-3">
    <%= f.label :address, 'Address' %><br>
    <%= f.text_field :address %>
  </div>
  <div class="col-md-3">
    <%= f.label :city, 'City' %><br>
    <%= f.text_field :city %>
  </div>
  <div class="col-md-3">
    <%= f.label :state_id, 'State' %><br>
    <%= f.number_field :state_id %>
  </div>
  <div class="col-md-3">
    <%= f.label :zip, 'Zip' %><br>
    <%= f.text_field :zip %>
  </div>
  <div class="col-md-3">
    <%= f.label :isfinancialinstitution, 'Financial Institution?' %><br>
    <%= f.check_box :isfinancialinstitution %>
  </div>
  <div class="col-md-3">
    <%= f.label :isoutofstatpac, 'Out of State PAC?' %><br>
    <%= f.check_box :isoutofstatpac %>
  </div>
  <div class="col-md-3">
    <%= f.label :pacid, 'PAC ID #' %><br>
    <%= f.text_field :pacid %>
  </div>
  <div class="col-md-3">
    <%= f.label :amount, 'Amount' %><br>
    <%= f.text_field :amount %>
  </div>
  <div class="col-md-3">
    <%= f.label :interestrate, 'Interest Rate' %><br>
    <%= f.text_field :interestrate %>
  </div>
  <div class="col-md-3">
    <%= f.label :date, 'Date' %><br>
    <%= f.date_select :date %>
  </div>
  <div class="col-md-3">
    <%= f.label :maturitydate, 'Maturity Date' %><br>
    <%= f.date_select :maturitydate %>
  </div>
  <div class="col-md-3">
    <%= f.label :collateraldescription, 'Collateral Description' %><br>
    <%= f.text_area :collateraldescription %>
  </div>
  <div class="col-md-3">
    <%= f.label :ispersonalfunddeposit, 'Personal Fund Deposit?' %><br>
    <%= f.check_box :ispersonalfunddeposit %>
  </div>
</div>
</div>
<%= link_to_remove_association "Remove", f %>
</div>
