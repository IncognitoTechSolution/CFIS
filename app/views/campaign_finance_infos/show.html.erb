            <tr>
              <th>Prefix</th>
              <th>Entity type</th>
              <th>Lendername</th>
              <th>Address</th>
              <th>City</th>
              <th>State</th>
              <th>Zip</th>
              <th>Isfinancialinstitution</th>
              <th>Isoutofstatpac</th>
              <th>Pacid</th>
              <th>Amount</th>
              <th>Interestrate</th>
              <th>Date</th>
              <th>Maturitydate</th>
              <th>Collateraldescription</th>
              <th>Ispersonalfunddeposit</th>
              <th colspan="3"></th>
            </tr>
            </thead>
            <tbody>
            <tr>
              <td><%= loan.prefix_id %></td>
              <td><%= loan.entity_type_id %></td>
              <td><%= loan.lendername %></td>
              <td><%= loan.address %></td>
              <td><%= loan.city %></td>
              <td><%= loan.state_id %></td>
              <td><%= loan.zip %></td>
              <td><%= loan.isfinancialinstitution %></td>
              <td><%= loan.isoutofstatpac %></td>
              <td><%= loan.pacid %></td>
              <td><%= loan.amount %></td>
              <td><%= loan.interestrate %></td>
              <td><%= loan.date %></td>
              <td><%= loan.maturitydate %></td>
              <td><%= loan.collateraldescription %></td>
              <td><%= loan.ispersonalfunddeposit %></td>
            </tr>
            </tbody>
        <% end %>
        </table>
    <% else %>
        <h5>No Loans For This Report</h5>
    <% end %>
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
