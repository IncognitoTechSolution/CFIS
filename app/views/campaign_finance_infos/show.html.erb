<p id="notice"><%= notice %></p>
<div class ="row">
  <div class="col-md-5"><p>
  <strong>Report Name:</strong>
  <%= @campaign_finance_info.filename %>
</p></div>
  <div class="col-md-5"><p>
  <strong>Filer:</strong>
  <%= @campaign_finance_info.filer_id %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Report Type:</strong>
  <%= @campaign_finance_info.report_type_id %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Candidate:</strong>
  <%= @campaign_finance_info.candidate_id %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Treasurer:</strong>
  <%= @campaign_finance_info.treasurer_id %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Election Type:</strong>
  <%= @campaign_finance_info.election_type_id %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Sudmit Date:</strong>
  <%= @campaign_finance_info.sudmitdate %>
</p></div>
  <div class="col-md-5"><p>
  <strong>Campaign Year:</strong>
  <%= @campaign_finance_info.campaignyear %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Ontime:</strong>
  <%= @campaign_finance_info.ontime %>
</p></div>
  <div class="col-md-5"><p>
  <strong>Periodbegin:</strong>
  <%= @campaign_finance_info.periodbegin %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Periodend:</strong>
  <%= @campaign_finance_info.periodend %>
</p></div>

  <div class="col-md-5"><p>
  <strong>Electiondate:</strong>
  <%= @campaign_finance_info.electiondate %>
</p></div>
</div>

--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
<table>
  <% if @campaign_finance_info.committee_notices.present? %>
      <h4>Notice From Committee</h4>
      <% @campaign_finance_info.committee_notices.each do |committee_notice| %>
          <thead>
          <tr>
            <th>Name</th>
            <th>Committee Type</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th colspan="3"></th>
          </tr>
          </thead>
          <tbody>
          <tr>
            <td><%= committee_notice.name %></td>
            <td><%= committee_notice.committee_type_id %></td>
            <td><%= committee_notice.address %></td>
            <td><%= committee_notice.city %></td>
            <td><%= committee_notice.state_id %></td>
            <td><%= committee_notice.zip %></td>
          </tr>
          </tbody>
      <% end %>
      </table>
  <% else %>
      <h5>No Notice From Committee(s)</h5>
  <% end %>
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
<table>
  <% if @campaign_finance_info.contributions.present? %>
      <h4>Contributions</h4>
      <% @campaign_finance_info.contributions.each do |contribution| %>
          <thead>
          <tr>
            <th>Date</th>
            <th>Contributor Type</th>
            <th>Prefix</th>
            <th>Contributor Name</th>
            <th>Out of State PAC</th>
            <th>PAC ID #</th>
            <th>Contributor Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip Code</th>
            <th>Amount</th>
            <th>Description</th>
            <th colspan="3"></th>
          </tr>
          </thead>
          <tbody>
          <tr>
            <td><%= contribution.date  %></td>
            <td><%= contribution.contribution_type_id %></td>
            <td><%= contribution.prefix_id %></td>
            <td><%= contribution.contributorname %></td>
            <td><%= contribution.isoutofstatepac %></td>
            <td><%= contribution.pacid %></td>
            <td><%= contribution.address %></td>
            <td><%= contribution.city %></td>
            <td><%= contribution.state_id %></td>
            <td><%= contribution.zip %></td>
            <td><%= contribution.amount %></td>
            <td><%= contribution.description %></td>
          </tr>
          </tbody>
      <% end %>
      </table>
  <% else %>
      <h5>No Contributions For This Report</h5>
  <% end %>
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
<table>
  <% if @campaign_finance_info.expenditures.present? %>
      <h4>Expenditures</h4>
      <% @campaign_finance_info.expenditures.each do |expenditure| %>
  <thead>
  <tr>
    <th>Expend type</th>
    <th>Expend category</th>
    <th>Payment method</th>
    <th>Prefix</th>
    <th>Entity type</th>
    <th>Contributorname</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Amount</th>
    <th>Date</th>
    <th>Description</th>
    <th>Isbenefitcoh</th>
    <th>Istraveloutsidetx</th>
    <th>Isreimbursementintended</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= expenditure.expend_type_id %></td>
        <td><%= expenditure.expend_category_id %></td>
        <td><%= expenditure.payment_method_id %></td>
        <td><%= expenditure.prefix_id %></td>
        <td><%= expenditure.entity_type_id %></td>
        <td><%= expenditure.contributorname %></td>
        <td><%= expenditure.address %></td>
        <td><%= expenditure.city %></td>
        <td><%= expenditure.state_id %></td>
        <td><%= expenditure.zip %></td>
        <td><%= expenditure.amount %></td>
        <td><%= expenditure.date %></td>
        <td><%= expenditure.description %></td>
        <td><%= expenditure.isbenefitCOH %></td>
        <td><%= expenditure.istraveloutsidetx %></td>
        <td><%= expenditure.isreimbursementintended %></td>
      </tr>
  </tbody>
  <% end %>
</table>
  <% else %>
      <h5>No Expenditure For This Report</h5>
  <% end %>
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
<table>
    <% if @campaign_finance_info.loans.present? %>
        <h4>Loans</h4>
        <% @campaign_finance_info.loans.each do |loan| %>
