<p id="notice"><%= notice %></p>

<h1>Listing Investment Purchases</h1>

<table>
  <thead>
    <tr>
      <th>Campaign finance info</th>
      <th>Prefix</th>
      <th>Entity type</th>
      <th>Investorname</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Amount</th>
      <th>Date</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @investment_purchases.each do |investment_purchase| %>
      <tr>
        <td><%= investment_purchase.campaign_finance_info_id %></td>
        <td><%= investment_purchase.prefix_id %></td>
        <td><%= investment_purchase.entity_type_id %></td>
        <td><%= investment_purchase.investorname %></td>
        <td><%= investment_purchase.address %></td>
        <td><%= investment_purchase.city %></td>
        <td><%= investment_purchase.state_id %></td>
        <td><%= investment_purchase.zip %></td>
        <td><%= investment_purchase.amount %></td>
        <td><%= investment_purchase.date %></td>
        <td><%= investment_purchase.description %></td>
        <td><%= link_to 'Show', investment_purchase %></td>
        <td><%= link_to 'Edit', edit_investment_purchase_path(investment_purchase) %></td>
        <td><%= link_to 'Destroy', investment_purchase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Investment purchase', new_investment_purchase_path %>
